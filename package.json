{
  "name": "leaseup",
  "private": true,
  "scripts": {
    "generate": "turbo run generate",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "test:e2e:ui": "cd packages/e2e-tests && pnpm test:ui",
    "test:e2e:headed": "cd packages/e2e-tests && pnpm test:headed",
    "test:e2e:auth": "cd packages/e2e-tests && pnpm test:auth",
    "postinstall": "turbo run generate"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.4",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
